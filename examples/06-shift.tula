let Callback { Halt Shift }

run (Print Shift) { & 69 420 1337 1 2 3 & }

for c in Callback case (Print c) & do & ! c
case Shift & do & -> Pick

for s     in Integer case Pick        s do s -> (Pick s)
for s t   in Integer case (Pick s)    t do t <- (Place s t)
for s t _ in Integer case (Place s t) _ do t -> (Skip s)
for s t   in Integer case (Skip s)    t do t -> (Pick s)
for s     in Integer case (Pick s)    & do & <- (Finish s)
for s _   in Integer case (Finish s)  _ do s -> (Print Halt)
